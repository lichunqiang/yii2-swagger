{
  "name": "light/yii2-swagger",
  "description": "swagger intergation with yii2",
  "type": "library",
  "keywords": ["yii2","extension","swagger","Restful","document"],
  "license": "MIT",
  "authors": [
    {
      "name": "lichunqiang",
      "email": "light-li@hotmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.0",
    "zircote/swagger-php": "^4.0",
    "bower-asset/swagger-ui": "^4.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "light\\swagger\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
